{
  "name": "@fig/plugins",
  "version": "1.1.5",
  "description": "",
  "main": "dist/cjs/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/esm/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "tsc --noEmit",
    "clean": "rm -rf dist",
    "build": "yarn clean && yarn build:cjs && yarn build:esm && cp index.d.ts dist/",
    "build:cjs": "tsc --declaration --outDir dist/cjs",
    "build:esm": "tsc --declaration --module esnext --outDir dist/esm",
    "preupdate-plugins": "tsc --build tsconfig.update.json",
    "update-plugins": "node dist/cjs/update-plugins.js",
    "postupdate-plugins": "npm run clean"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/withfig/plugins.git"
  },
  "author": "Fig",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/withfig/plugins/issues"
  },
  "homepage": "https://github.com/withfig/plugins",
  "devDependencies": {
    "axios": "^0.26.0",
    "dotenv": "^16.0.0",
    "ts-node": "^10.6.0",
    "typescript": "^3.5.3"
  }
}
